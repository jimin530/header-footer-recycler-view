        </activity>
        <activity android:name="com.willblaschko.example.ExampleHorizontalLinearLayoutActivity"/>
        <activity android:name="com.willblaschko.example.ExampleVerticalLinearLayoutActivity"/>
        <activity android:name="com.willblaschko.example.ExampleHorizontalGridActivity"/>
        <activity android:name="com.willblaschko.example.ExampleVerticalGridActivity"/>
        <activity android:name="com.willblaschko.example.ExampleHorizontalStaggeredGridActivity"/>
        <activity android:name="com.willblaschko.example.ExampleVerticalStaggeredGridActivity"/>


    </application>

</manifest>
